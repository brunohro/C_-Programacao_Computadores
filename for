#include <stdio.h>

int main(void)
{
    float n1, n2, n3;
    for(int i = 1; i <= 3; i++){
        printf("Digite a nota 1 do usuario: \n");
        scanf("%f", &n1);
        printf("Digite a nota 2 do usuario: \n");
        scanf("%f", &n2);
        printf("Digite a nota 3 do usuario: \n");
        scanf("%f", &n3);

        printf("SUA MEDIA FOI DE %.2f", (n1 + n2 + n3) / 3);
        printf
    }
    return 0;
}
